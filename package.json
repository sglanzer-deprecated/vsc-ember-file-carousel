{
    "name": "vsc-ember-file-carousel",
    "displayName": "vsc-ember-file-carousel",
    "description": "Cycle between related Ember files",
    "version": "0.0.1",
    "publisher": "sglanzer",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
      "onCommand:extension.emberFileCarousel"
    ],
    "main": "./out/src/extension",
    "contributes": {
      "commands": [
          {
            "command": "extension.emberFileCarousel",
            "title": "Ember file carousel"
          }
      ],
      "keybindings": [
          {
            "mac": "cmd+shift+r",
            "win": "ctrl+shift+r",
            "linux": "ctrl+shift+r",
            "key": "ctrl+shift+r",
            "command": "extension.emberFileCarousel"
          }
      ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
